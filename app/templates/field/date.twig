<label for="{{ field.slug }}Input">{{ field.name }}</label>
{% if result is defined %}
    <label id="{{ field.slug }}">{{ result.value[field.slug] }}</label>
{% else %}
    <div class="col-sm-12">
        <input class="form-control" type="date" name="{{ field.slug }}" id="{{ field.slug }}">
    </div>
{% endif %}

